{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400"
                }
            ]
        }
    ],
    "rewrites": [
            {
                "source": "/(.*)",
                "destination": "/index.html"
            }
        ]
    }